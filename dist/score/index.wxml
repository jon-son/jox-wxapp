<!--components/parts/score/index.wxml-->
<view class='jox-class'>
  <view class='score {{type}}-radius'>
    <view class='score-text' wx:for="{{scoreData}}" wx:key="index">
      <text class='score-value'>{{item.score}}</text>
      <text class='score-name'>{{item.title}}</text>
    </view>
  </view>
</view>