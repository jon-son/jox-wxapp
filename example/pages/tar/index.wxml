<!--pages/test/test.wxml-->

<view class='jox-title'>一、使用方法：</view>
<view class='jox-content'>json文件引用</view>
<jox-code codeType='html' codeData="{{jsonData}}" />
<view class='jox-code'></view>
<view class='jox-content'>wxml文件引用</view>
<jox-code codeType='html' codeData="{{wxmlData}}" />
<view class='jox-content'>js文件引用</view>
<jox-code codeType='html' codeData="{{jsData}}" />
<view class='jox-title'>二、属性介绍</view>
<jox-table tableX="3" tableY="7" flex="center" tableData="{{tableData}}" />

<view class='jox-title'>三、效果展示</view>

<jox-tar tarList="{{tar}}" fixed="{{true}}" bindjoxtarselected='joxTarTap'>
  <view slot="tarItem">{{tar_id}}</view>
</jox-tar>